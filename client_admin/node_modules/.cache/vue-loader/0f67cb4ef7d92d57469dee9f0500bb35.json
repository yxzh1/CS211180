r\n\t\t\t\t\tconsole.error(json.error);\r\n\t\t\t\t}\r\n\t\t\t},\r\n\r\n\t\t},\r\n\t\tcreated() {\r\n\t\t\tthis.get_artcle_type();\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style>\r\n\t.bg {\r\n\t\tbackground-color: #FFFFFF;\r\n\t}\r\n\r\n\t.avatar-uploader .el-upload {\r\n\t\tborder: 1px dashed #d9d9d9;\r\n\t\tborder-radius: 6px;\r\n\t\tcursor: pointer;\r\n\t\tposition: relative;\r\n\t\toverflow: hidden;\r\n\t}\r\n\r\n\t.avatar-uploader .el-upload:hover {\r\n\t\tborder-color: #409EFF;\r\n\t}\r\n\r\n\t.avatar-uploader-icon {\r\n\t\tfont-size: 28px;\r\n\t\tcolor: #8c939d;\r\n\t\twidth: 178px;\r\n\t\theight: 178px;\r\n\t\tline-height: 178px;\r\n\t\ttext-align: center;\r\n\t}\r\n\r\n\t.avatar {\r\n\t\twidth: 178px;\r\n\t\theight: 178px;\r\n\t\tdisplay: block;\r\n\t}\r\n</style>\r\n"]}]}