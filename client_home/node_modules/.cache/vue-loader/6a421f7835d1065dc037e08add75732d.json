{"remainingRequest":"D:\\WorkSpace\\localCode\\CS211180\\client_home\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!D:\\WorkSpace\\localCode\\CS211180\\client_home\\src\\views\\index.vue?vue&type=script&lang=js&","dependencies":[{"path":"D:\\WorkSpace\\localCode\\CS211180\\client_home\\src\\views\\index.vue","mtime":1710236176058},{"path":"D:\\WorkSpace\\localCode\\CS211180\\client_home\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\WorkSpace\\localCode\\CS211180\\client_home\\node_modules\\babel-loader\\lib\\index.js","mtime":315532800000},{"path":"D:\\WorkSpace\\localCode\\CS211180\\client_home\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\WorkSpace\\localCode\\CS211180\\client_home\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkNA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <div class=\"page_root\" id=\"root_demo\">\r\n    <div class=\"warp\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col\">\r\n            <div class=\"card_ad\">\r\n              <!-- 页头 -->\r\n              <list_ad\r\n                  :list=\"list_ad\"\r\n                  location=\"店招\"\r\n                  v-if=\"$check_action('/ad/list', 'get')\"\r\n                  style=\"marginbottom: 0.5rem\"\r\n              />\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"warp\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <div class=\"sw_no\">\r\n              <!-- 轮播图组件 -->\r\n              <div class=\"swiper_img\">\r\n                <swiper_img :list=\"list_slide\"/>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"warp\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col\">\r\n            <div class=\"card_ad\">\r\n              <!-- 顶部 -->\r\n              <list_ad\r\n                  :list=\"list_ad\"\r\n                  v-if=\"$check_action('/ad/list', 'get')\"\r\n                  location=\"顶部广告\"\r\n              />\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"warp\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col\">\r\n            <div class=\"card_ad\">\r\n              <!-- 中部 -->\r\n              <list_ad\r\n                  :list=\"list_ad\"\r\n                  v-if=\"$check_action('/ad/list', 'get')\"\r\n                  location=\"中部广告\"\r\n              />\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"warp\">\r\n      <!-- 容器 -->\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <!-- 标题栏组件 -->\r\n            <div class=\"article_recommend\">\r\n              <div class=\"title\">\r\n                <span>新闻资讯 </span>\r\n              </div>\r\n              <div class=\"more_box\">\r\n                <router-link to=\"/article/list\" class=\"more\">\r\n                  <span>更多</span>\r\n                </router-link>\r\n              </div>\r\n              <div class=\"switch\" @click=\"show_list_article = !show_list_article\">\r\n                <div class=\"switch_box\">\r\n                  <span v-if=\"show_list_article\"> 切换表格 </span>\r\n                  <span v-else> 切换列表 </span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <!--新闻资讯列表组件 -->\r\n            <list_article v-if=\"show_list_article\" :list=\"list_article\"/>\r\n            <div class=\"overflow-auto\" v-else>\r\n              <table id=\"list_article\" role=\"table\" aria-busy=\"false\"\r\n                     :aria-colcount=\"fields_article.length\"\r\n                     class=\"table b-table table-striped table-hover\">\r\n                <thead>\r\n                <tr>\r\n                  <th v-for=\"(o,i) in fields_article\" :key=\"i\">\r\n                    {{ o.label }}\r\n                  </th>\r\n                </tr>\r\n                </thead>\r\n                <tbody>\r\n                <tr v-for=\"(o, i) in list_table_article\" :key=\"i\">\r\n                  <td v-for=\"(oj,n) in fields_goods\" @click=\"to_details('goods',o)\">\r\n                    <img v-if=\"oj.type && oj.type == '图片' \" :src=\"$fullUrl(o[oj.key])\" alt=\"\"\r\n                         v-default-img=\"'/img/default.png'\">\r\n                    <span v-else>{{ o[oj.key] }}</span>\r\n                  </td>\r\n                </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"warp\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col\">\r\n            <div class=\"card_ad\">\r\n              <!-- 底部 -->\r\n              <list_ad\r\n                  :list=\"list_ad\"\r\n                  v-if=\"$check_action('/ad/list', 'get')\"\r\n                  location=\"底部广告\"\r\n              />\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 推荐招聘信息模块(开始) -->\r\n    <div class=\"warp\">\r\n      <!-- 容器 -->\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <!-- 标题栏组件 -->\r\n            <div class=\"diy_recommend\">\r\n              <div class=\"title\">\r\n                <span>招聘信息推荐 </span>\r\n              </div>\r\n              <div class=\"more_box\">\r\n                <router-link to=\"/recruitment_information/list\" class=\"more\">\r\n                  <span>更多</span>\r\n                </router-link>\r\n              </div>\r\n              <div class=\"switch\" @click=\"show_list_recruitment_information = !show_list_recruitment_information\">\r\n                <div class=\"switch_box\">\r\n                  <span v-if=\"show_list_recruitment_information\"> 切换表格 </span>\r\n                  <span v-else> 切换列表 </span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <!--招聘信息推荐列表组件 -->\r\n            <list_recruitment_information v-if=\"show_list_recruitment_information\"\r\n                                          :list=\"list_recruitment_information\"/>\r\n            <div class=\"overflow-auto\" v-else>\r\n              <table id=\"list_diy\" role=\"table\" aria-busy=\"false\"\r\n                     :aria-colcount=\"fields_recruitment_information.length\"\r\n                     class=\"table b-table table-striped table-hover\">\r\n                <thead>\r\n                <tr>\r\n                  <th v-for=\"(o,i) in fields_recruitment_information\" :key=\"i\">\r\n                    {{ o.label }}\r\n                  </th>\r\n                </tr>\r\n                </thead>\r\n                <tbody>\r\n                <tr v-for=\"(o, i) in list_table_recruitment_information\" :key=\"i\"\r\n                    @click=\"to_details('recruitment_information',o,'recruitment_information_id')\">\r\n                  <td v-for=\"(oj,n) in fields_recruitment_information\">\r\n                    <img v-if=\"oj.type && oj.type == '图片' \" :src=\"$fullUrl(o[oj.key])\" alt=\"\"\r\n                         v-default-img=\"'/img/default.png'\">\r\n                    <span v-else>{{ o[oj.key] }}</span>\r\n                  </td>\r\n                </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- 推荐招聘信息模块(结束) -->\r\n\r\n    <div class=\"warp\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col\">\r\n            <div class=\"card_link\">\r\n              <!-- 底部友情链接 -->\r\n              <list_link\r\n                  v-if=\"$check_action('/link/list', 'get')\"\r\n                  :list=\"list_link\"\r\n              />\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport mixin from \"@/mixins/page.js\";\r\nimport list_recruitment_information from \"@/components/diy/list_recruitment_information.vue\";\r\nimport bar_title from \"@/components/diy/bar_title.vue\";\r\nimport list_article from \"@/components/diy/list_article.vue\";\r\nimport swiper_img from \"@/components/diy/swiper_img.vue\";\r\nimport list_ad from \"@/components/diy/list_ad.vue\";\r\nimport list_link from \"@/components/diy/list_link.vue\";\r\nimport swiper_notice from \"@/components/diy/swiper_notice.vue\";\r\n\r\nexport default {\r\n  mixins: [mixin],\r\n  components: {\r\n    list_recruitment_information,\r\n    bar_title,\r\n    list_article,\r\n    swiper_img,\r\n    swiper_notice,\r\n    list_ad,\r\n    list_link,\r\n  },\r\n  data() {\r\n    return {\r\n      isSmall: false,\r\n      sendValue: \"\",\r\n      chatList: [],\r\n      showChat: false,\r\n      isAdmin: false,\r\n      token: \"\",\r\n      // 文章模型数组\r\n      list_article: [],\r\n      vm_arr_article: [\r\n        \"img\",\r\n        \"title\",\r\n        \"description\",\r\n        \"hits\",\r\n        \"type\",\r\n        \"tag\",\r\n        \"source\",\r\n        \"url\",\r\n        \"create_time\",\r\n      ],\r\n      fields_article: [\r\n        {key: \"img\", label: \"图片\"},\r\n        {key: \"title\", label: \"标题\"},\r\n        {key: \"description\", label: \"描述\"},\r\n        {key: \"hits\", label: \"点击量\"},\r\n        {key: \"type\", label: \"分类\"},\r\n        {key: \"tag\", label: \"标签\"},\r\n        {key: \"source\", label: \"来源\"},\r\n        {key: \"url\", label: \"来源地址\"},\r\n        {key: \"create_time\", label: \"发布时间\"},\r\n      ],\r\n      show_list_article: true,\r\n      show_list_recruitment_information: true,\r\n      list_recruitment_information: [],\r\n      vm_arr_recruitment_information: [\r\n        \"recruitment_information_id\",\r\n        \"enterprise_name\",\r\n        \"business_address\",\r\n        \"enterprise_industry\",\r\n        \"enterprise_picture\",\r\n        \"person_in_charge\",\r\n        \"company_profile\",\r\n        \"professional_requirements\",\r\n        \"age_requirements\",\r\n        \"character\",\r\n        \"educational_requirements\",\r\n        \"graduation_school_requirements\",\r\n        \"recruitment_occupation\",\r\n        \"certificate_requirements\",\r\n        \"occupational_category\",\r\n        \"type_of_work\",\r\n        \"working_province\",\r\n        \"working_years\",\r\n        \"a_monthly_salary\",\r\n        \"is_it_wrapped\",\r\n        \"working_hours\",\r\n        \"compensation_and_benefits\",\r\n        \"job_description\",\r\n      ],\r\n      fields_recruitment_information: [\r\n        {\r\n          key: \"enterprise_name\",\r\n          label: \"企业名称\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"business_address\",\r\n          label: \"企业地址\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"enterprise_industry\",\r\n          label: \"企业行业\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"enterprise_picture\",\r\n          label: \"企业图片\",\r\n          type: \"图片\"\r\n        },\r\n        {\r\n          key: \"person_in_charge\",\r\n          label: \"负责人\",\r\n          type: \"UID\"\r\n        },\r\n        {\r\n          key: \"company_profile\",\r\n          label: \"企业简介\",\r\n          type: \"多文本\"\r\n        },\r\n        {\r\n          key: \"professional_requirements\",\r\n          label: \"专业要求\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"age_requirements\",\r\n          label: \"年龄要求\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"character\",\r\n          label: \"性格\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"educational_requirements\",\r\n          label: \"学历要求\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"graduation_school_requirements\",\r\n          label: \"毕业学校要求\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"recruitment_occupation\",\r\n          label: \"招聘职业\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"certificate_requirements\",\r\n          label: \"证书要求\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"occupational_category\",\r\n          label: \"职业类别\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"type_of_work\",\r\n          label: \"工作类型\",\r\n          type: \"下拉\"\r\n        },\r\n        {\r\n          key: \"working_province\",\r\n          label: \"工作省份\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"working_years\",\r\n          label: \"工作年限\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"a_monthly_salary\",\r\n          label: \"月薪\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"is_it_wrapped\",\r\n          label: \"是否包住\",\r\n          type: \"下拉\"\r\n        },\r\n        {\r\n          key: \"working_hours\",\r\n          label: \"工作时间\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"compensation_and_benefits\",\r\n          label: \"薪酬福利\",\r\n          type: \"文本\"\r\n        },\r\n        {\r\n          key: \"job_description\",\r\n          label: \"职业描述\",\r\n          type: \"多文本\"\r\n        },\r\n      ],\r\n      list_slide: [],\r\n      list_menu: [],\r\n      list_link: [],\r\n      list_notice: [],\r\n      list_ad: []\r\n    };\r\n  },\r\n  created() {\r\n  },\r\n  methods: {\r\n    // toggle\r\n    toToggle() {\r\n      this.isAdmin = !this.isAdmin;\r\n    },\r\n    // 获取招聘信息列表\r\n    get_recruitment_information() {\r\n      let url = \"~/api/recruitment_information/get_list?\";\r\n      this.$get(url, {\r\n        \"page\": 1,\r\n        \"size\": 6\r\n      }, (json) => {\r\n        if (json.result) {\r\n          this.list_recruitment_information = json.result.list;\r\n        }\r\n      })\r\n    },\r\n\r\n    /**\r\n     * 当前年月日时分秒方法\r\n     * @param {Object} fmt\r\n     */\r\n    dateFormat(fmt) {\r\n      var myDate = new Date();\r\n      var o = {\r\n        \"M+\": myDate.getMonth() + 1, // 月份\r\n        \"d+\": myDate.getDate(), // 日\r\n        \"h+\": myDate.getHours(), // 小时\r\n        \"m+\": myDate.getMinutes(), // 分\r\n        \"s+\": myDate.getSeconds(), // 秒\r\n        \"q+\": Math.floor((myDate.getMonth() + 3) / 3), // 季度\r\n        \"S\": myDate.getMilliseconds() // 毫秒\r\n      };\r\n      if (/(y+)/.test(fmt))\r\n        fmt = fmt.replace(RegExp.$1, (myDate.getFullYear() + \"\").substr(4 - RegExp.$1.length));\r\n      for (var k in o)\r\n        if (new RegExp(\"(\" + k + \")\").test(fmt)) fmt = fmt.replace(RegExp.$1, (RegExp.$1.length == 1) ? (o[k]) : ((\"00\" + o[k]).substr((\"\" + o[k]).length)));\r\n      return fmt;\r\n    },\r\n\r\n    // 获取轮播图\r\n    get_slides() {\r\n      this.$get(\"~/api/slides/get_list?\", {}, (json) => {\r\n        if (json.result) {\r\n          this.list_slide = json.result.list;\r\n        }\r\n      });\r\n    },\r\n    // 获取导航栏\r\n    get_menu() {\r\n      var user_group = this.user_group;\r\n      this.$get(\r\n          \"~/api/auth/get_list?\",\r\n          {\r\n            page: \"\",\r\n            user_group,\r\n            get: 1,\r\n            position: \"top\"\r\n          },\r\n          (json) => {\r\n            if (json.result) {\r\n              this.list_menu = json.result.list;\r\n            }\r\n          }\r\n      );\r\n    },\r\n    // 获取文章\r\n    get_article() {\r\n      this.$get(\r\n          \"~/api/article/get_list?\",\r\n          {\r\n            page: 1,\r\n            size: 4,\r\n          },\r\n          (json) => {\r\n            if (json.result) {\r\n              var list_article = json.result.list;\r\n              this.list_article = list_article;\r\n            }\r\n          }\r\n      );\r\n    },\r\n    // 获取广告\r\n    get_ad() {\r\n      this.$get(\r\n          \"~/api/ad/get_list?\",\r\n          {\r\n            orderby: '`display` desc'\r\n          },\r\n          (json) => {\r\n            if (json.result) {\r\n              var list_ad = json.result.list;\r\n              this.list_ad = list_ad;\r\n            }\r\n          }\r\n      );\r\n    },\r\n    // 获取链接列表\r\n    get_link() {\r\n      this.$get(\r\n          \"~/api/link/get_list?\",\r\n          {\r\n            page: 1,\r\n            size: 8,\r\n          },\r\n          (json) => {\r\n            if (json.result) {\r\n              var list_link = json.result.list;\r\n              this.list_link = list_link;\r\n            }\r\n          }\r\n      );\r\n    },\r\n    // 获取公告列表\r\n    get_notice() {\r\n      this.$get(\r\n          \"~/api/notice/get_list?\",\r\n          {\r\n            page: 1,\r\n            size: 3,\r\n          },\r\n          (json) => {\r\n            if (json.result) {\r\n              var list_notice = json.result.list;\r\n              this.list_notice = list_notice;\r\n            }\r\n          }\r\n      );\r\n    },\r\n    to_details(key, o, id) {\r\n      if (!id) {\r\n        id = key + \"_id\";\r\n      }\r\n      this.$router.push('/' + key + '/details?' + id + '=' + o[id]);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.get_recruitment_information();\r\n    this.get_menu();\r\n    this.get_slides();\r\n    this.