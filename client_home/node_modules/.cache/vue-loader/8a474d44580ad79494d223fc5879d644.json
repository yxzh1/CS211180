ager,\r\n\t\t\tlist_notice_announcement\r\n\t\t},\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\turl_get_list: \"~/api/notice_announcement/get_list?like=0\",\r\n\r\n\t\t\t\t// 查询条件\r\n\t\t\t\tquery: {\r\n\t\t\t\t\tkeyword: \"\",\r\n\t\t\t\t\tpage: 1,\r\n\t\t\t\t\tsize: 10,\r\n\t\t\t\t\t\"announcement_title\": \"\", // 公告标题\r\n\t\t\t\t\t\"release_time\": \"\", // 发布时间\r\n\t\t\t\t},\r\n\r\n\t\t\t\t// 排序内容\r\n\t\t\t\tlist_sort: [{\r\n\t\t\t\t\t\tname: \"创建时间从高到低\",\r\n\t\t\t\t\t\tvalue: \"`create_time` desc\",\r\n\t\t\t\t\t},\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tname: \"创建时间从低到高\",\r\n\t\t\t\t\t\tvalue: \"`create_time` asc\",\r\n\t\t\t\t\t},\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tname: \"更新时间从高到低\",\r\n\t\t\t\t\t\tvalue: \"`update_time` desc\",\r\n\t\t\t\t\t},\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tname: \"更新时间从低到高\",\r\n\t\t\t\t\t\tvalue: \"`update_time` asc\",\r\n\t\t\t\t\t},\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tname: \"公告标题正序\",\r\n\t\t\t\t\t\tvalue: \"`announcement_title` asc\",\r\n\t\t\t\t\t},\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tname: \"公告标题倒序\",\r\n\t\t\t\t\t\tvalue: \"`announcement_title` desc\",\r\n\t\t\t\t\t},\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tname: \"发布时间正序\",\r\n\t\t\t\t\t\tvalue: \"`release_time` asc\",\r\n\t\t\t\t\t},\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tname: \"发布时间倒序\",\r\n\t\t\t\t\t\tvalue: \"`release_time` desc\",\r\n\t\t\t\t\t},\r\n\t\t\t\t],\r\n\r\n        list: []\r\n\t\t\t}\r\n\t\t},\r\n\t\tmethods: {\r\n\r\n\t\t\t/**\r\n\t\t\t * 筛选选择\r\n\t\t\t */\r\n\t\t\tfilter_set(o,key) {\r\n\t\t\t    if (o == \"全部\") {\r\n\t\t\t        this.query[key] = \"\";\r\n\t\t\t    } else {\r\n\t\t\t        this.query[key] = o;\r\n\t\t\t    }\r\n\t\t\t    this.search();\r\n\t\t\t},\r\n\r\n\t\t\t/**\r\n\t\t\t * 排序\r\n\t\t\t */\r\n\t\t\tset_sort(o) {\r\n\t\t\t    this.query.orderby = o.value;\r\n\t\t\t    this.search();\r\n\t\t\t},\r\n\r\n\t\t\t/**\r\n\t\t\t * 筛选\r\n\t\t\t */\r\n\r\n\t\t\t/**\r\n\t\t\t * 重置\r\n\t\t\t */\r\n\t\t\treset() {\r\n\t\t\t\tthis.query.announcement_title = \"\"\r\n\t\t\t\tthis.query.release_time = \"\"\r\n\t\t\t\tthis.search();\r\n\t\t\t},\r\n\r\n\t\t\t// 返回条数\r\n\t\t\ttoSize(i){\r\n\t\t\t\tthis.query.size = i;\r\n\t\t\t\tthis.first();\r\n\t\t\t},\r\n\r\n\t\t\t// 返回页数\r\n\t\t\ttoPage(i){\r\n\t\t\t\tthis.query.page = i;\r\n\t\t\t\tthis.first();\r\n\t\t\t},\r\n\t\t},\r\n\t\tcomputed: {\r\n\t\t},\r\n\t\tcreated() {\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"]}]}