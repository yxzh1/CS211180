n                <b-pagination\r\n                  v-model=\"page_result\"\r\n                  :total-rows=\"count\"\r\n                  :per-page=\"size\"\r\n                  @change=\"jumpTo\"\r\n                ></b-pagination>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport mixin from \"@/mixins/page.js\";\r\nimport list_result_search from \"@/components/diy/list_result_search.vue\";\r\n\r\nexport default {\r\n  mixins: [mixin],\r\n  data() {\r\n    return {\r\n      query: {\r\n        word: \"\",\r\n        search_key: \"\",\r\n        search_type: \"\",\r\n        search_title: \"\",\r\n      },\r\n      search_val: \"\",\r\n      list_result: [],\r\n      page_result: 1,\r\n      size: 10,\r\n      count: 0,\r\n    };\r\n  },\r\n  methods: {\r\n    /**\r\n     *  获得结果\r\n     * */\r\n    get_result(page_result = 1) {\r\n      if (word == \"\") {\r\n        return;\r\n      }\r\n      var params = {};\r\n      var { word, search_key, search_type } = this.query;\r\n      params[search_key] = word;\r\n      params.page = page_result;\r\n      params.size = this.size;\r\n      this.$get(\"~/api/\" + search_type + \"/get_list?like=0\", params, (res) => {\r\n        if (res.result) {\r\n          var list_result = res.result.list || [];\r\n          list_result.map((o) => {\r\n            var title = o[search_key];\r\n            o.title =title;\r\n          });\r\n          this.list_result = list_result\r\n          this.count = res.result.count;\r\n        }\r\n      });\r\n    },\r\n    /**\r\n     *  跳转\r\n     * */\r\n    jumpTo(page_result) {\r\n      this.get_result(page_result);\r\n    },\r\n\r\n    /**\r\n     *  搜索\r\n     * */\r\n    submit_search() {\r\n      this.query.word = this.search_val\r\n      this.get_result();\r\n    },\r\n    /**\r\n     *  获取地址栏参数\r\n     * */\r\n    getQueryParams(){\r\n      for (const key in this.query) {\r\n        this.query[key] = this.$route.query[key]\r\n      }\r\n    }\r\n  },\r\n  components: { list_result_search },\r\n  mounted() {\r\n    this.getQueryParams();\r\n    this.get_result();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.card_search {\r\n  text-align: center;\r\n}\r\n.card_result_search > .title {\r\n  text-align: center;\r\n  padding: 10px 0;\r\n}\r\n.pager {\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n.search_box {\r\n  display: flex;\r\n  margin-bottom: 1rem;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/search"}]}